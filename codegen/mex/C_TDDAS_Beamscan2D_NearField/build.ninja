# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = C:/Program Files/MATLAB/R2021b
startdir = C:/Users/ru121koh/Desktop/PA-akustische-kamera

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera\codegen\mex\C_TDDAS_Beamscan2D_NearField" -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera" -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera\codegen\mex\C_TDDAS_Beamscan2D_NearField\interface" -I "C:\Program Files\MATLAB\R2021b\extern\include" -I "." -DMODEL=C_TDDAS_Beamscan2D_NearField_mex -D__USE_MINGW_ANSI_STDIO=1 -fopenmp 

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -std=c++11 -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera\codegen\mex\C_TDDAS_Beamscan2D_NearField" -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera" -I "C:\Users\ru121koh\Desktop\PA-akustische-kamera\codegen\mex\C_TDDAS_Beamscan2D_NearField\interface" -I "C:\Program Files\MATLAB\R2021b\extern\include" -I "." -DMODEL=C_TDDAS_Beamscan2D_NearField_mex -D__USE_MINGW_ANSI_STDIO=1 -fopenmp 

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2021b\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -o "C_TDDAS_Beamscan2D_NearField_mex.mexw64" -llibemlrt -llibcovrt -llibut -llibmwmathutil -fopenmp -Wl,--version-script,C_TDDAS_Beamscan2D_NearField_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $ldflags

